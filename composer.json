{
  "name": "undabot/json-api-symfony",
  "description": "",
  "type": "library",
  "require": {
    "php": "^7.3",
    "undabot/json-api-core": "dev-master@dev",
    "symfony/http-kernel": "^4.2",
    "symfony/validator": "^4.2",
    "symfony/orm-pack": "^1.0",
    "doctrine/annotations": "^1.6",
    "symfony/property-access": "^4.2",
    "symfony/serializer": "^4.2",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "phpstan/phpstan-phpunit": "^0.11.0",
    "phpstan/phpstan": "^0.11.1",
    "phpunit/phpunit": "^7.5"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.com:undabot/json-api-core.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Undabot\\SymfonyJsonApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Undabot\\SymfonyJsonApi\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": [
      "php-cs-fixer fix --diff --ansi --dry-run"
    ],
    "qc": [
      "phpstan analyse -n --ansi --no-progress"
    ],
    "test": [
      "phpunit"
    ]
  }
}
